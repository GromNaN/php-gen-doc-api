{
    "name": "zckrs/php-gen-doc-api",
    "type": "library",
    "description": "Generate documentation for php API based application. No dependency. No framework required.",
    "keywords": [
        "php",
        "api",
        "documentation",
        "generator",
        "agnostic"
    ],
    "homepage": "https://github.com/zckrs/php-gen-doc-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehdy Dara",
            "email": "mdara@eleven-labs.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "satooshi/php-coveralls": "0.7.*@dev",
        "phpunit/phpcov": "2.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Zckrs\\GenDocApi\\": "src/"
        }
    }
}
